{
  "name": "yamswap-core",
  "author": "a186r",
  "version": "1.0.0",
  "description": "core contract for the Yamswap protocol",
  "main": "truffle-config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YamSwap/yamswap-core.git"
  },
  "scripts": {
    "lint": "yarn prettier ./test/*.ts --check",
    "lint:fix": "yarn prettier ./test/*.ts --write",
    "clean": "rimraf ./build/",
    "precompile": "yarn clean",
    "compile": "waffle .waffle.json",
    "build": "waffle",
    "pretest": "yarn compile",
    "test": "mocha",
    "prepublishOnly": "yarn test"
  },
  "keywords": [
    "yamswap"
  ],
  "bugs": {
    "url": "https://github.com/YamSwap/yamswap-core/issues"
  },
  "files": [
    "contracts",
    "build"
  ],
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.2.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^8.2.3",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethereumjs-util": "^7.1.0",
    "ethers": "^5.4",
    "mocha": "^9.0.2",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "solc": "0.8.6",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "homepage": "https://github.com/YamSwap/yamswap-core#readme",
  "license": "MIT"
}
